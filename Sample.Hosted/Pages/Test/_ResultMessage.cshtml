@model dynamic

<div class="alert alert-success alert-dismissible fade show" role="alert">
    <h5 class="alert-heading">Request Successful!</h5>
    <hr>
    @if (!string.IsNullOrEmpty(Model.Message))
    {
        <p><strong>Message/Query:</strong> @Model.Message</p>
    }
    <p><strong>Token Validation:</strong> @Model.TokenValidation</p>
    <p class="mb-0"><small class="text-muted">Generated at: @Model.Timestamp</small></p>
    <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
</div> 