{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "doc": "https://github.com/dotnet/docs/issues/34726",
  "profiles": {
    "Kestrel": {
      "doc": "https://learn.microsoft.com/en-us/aspnet/core/fundamentals/environments",
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:5998;",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HTTP_PORTS": "5998;",
        "ASPNETCORE_HTTPS_PORT": "5999"
      }
    },
    "Docker": {
      "doc": "https://learn.microsoft.com/en-us/visualstudio/containers/container-launch-settings - not supported in rider yet",
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HTTP_PORT": "5998",
        "ASPNETCORE_HTTPS_PORT": "5999"
      },
      "useSSL": true,
      "sslPort": 5999
    }
  }
}